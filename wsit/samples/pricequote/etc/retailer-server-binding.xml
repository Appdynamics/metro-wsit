<bindings wsdlLocation="retailer.wsdl" xmlns="http://java.sun.com/xml/ns/jaxws" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb">
    <bindings node="wsdl:definitions">
        <package name="pricequote.retailer.server"/>
    </bindings>
    <bindings node="wsdl:definitions/wsdl:types/xsd:schema[@targetNamespace='http://example.org/retailer']"  >
        <jaxb:schemaBindings >
            <jaxb:package name="pricequote.retailer.server"/>
        </jaxb:schemaBindings>
    </bindings>
</bindings>

